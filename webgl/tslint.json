{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "src/generated/**/*",
      "node_modules/**/*.ts",
      "coverage/**/*"
    ]
  },
  "rules": {
    "jsx-no-lambda": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "prefer-for-of": false
  }
}
